	}
	$('#backtop').on('click', function () {
		$("html,body").animate({ "scrollTop": 0 }, 500);
	});
	global.backTop();
	$(window).scroll(function () {
		global.h = $(window).height();
		global.st = $(window).scrollTop();
		global.backTop();
	});
	$(window).resize(function () {
		global.h = $(window).height();
		global.st = $(window).scrollTop();
		global.backTop();
	})
</script>
